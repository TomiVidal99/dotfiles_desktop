# DEFINICIÓN VARIABLES
# impermeabilidad electrica del medio [F/m]
E0 = 8.854e-12;
# constante de fuerzas coulombianas.
K = 1 / (4*pi*E0);
# radio de bohr [m]
R0 = 0.53e-10;
# masa electrón [kg]
Me = 9.1e-31;
# carga del protón [C]
Qp = 1.602176634e-19;
# carga del electrón [C]
Qe = -Qp;
# constante de Boltzmann [eV/°K]
Kb = 8.617333262145e-5;
# constante de Planck [eV*s]
Hp = 4.135667696e-15;
# Velocidad de la luz [m/s]
C = 299792458;

# DEFINICION DE FUNCIONES COMUNES.
# Fermi-Dirac distribución de probabilidad de que un estado cuántico esté ocupado [unidad de volumen por unidad de energía].
FNFD = @(E, Ef, T) 1 / (1 + exp( (E-Ef) / (Kb*T) ));
